{
  "manifest_version": 2,
  "name": "Golden ratio series", 
  "version": "1.1",
  "description": "Get the golden ratios of a given value",
  "author": "Ryu Sung Chul",

  "icons": {
    "16": "icons/grs-v2-16.png",
    "32": "icons/grs-v2-32.png",
    "48": "icons/grs-v2-48.png",
    "128": "icons/grs-v2-128.png"
  },
  
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/grs-v2-16.png",
      "32": "icons/grs-v2-32.png",
      "48": "icons/grs-v2-48.png",
      "128": "icons/grs-v2-128.png"
    }
  },
  
  "permissions": ["storage"]
  
}

  